<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.wechat_ui">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="拾忆"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Sign_in">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MemoryEdit"/>
        <activity android:name=".MemoryRead"/>
        <activity android:name=".UserinfoAcitvity"/>
        <activity android:name=".user_memory_READ"/>
        <activity android:name=".user_memory_EDIT"/>
        <activity android:name=".list"/>
        <activity android:name=".About"/>
        <activity android:name=".registration"/>
        <activity android:name=".MainActivity"/>
    </application>

</manifest>